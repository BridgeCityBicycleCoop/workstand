{
    "parser": "babel-eslint",
    "plugins": [
        "react",
        "jsx-a11y"
    ],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/jsx-wrap-multilines": 0,
        "react/jsx-one-expression-per-line": 0,
        "arrow-parens": 0,
        "implicit-arrow-linebreak": 0,
    }
}